{
    "name": "maxwellwr/turnstile",
    "type": "phpbb-extension",
    "description": "CloudFlare Turnstile extension for phpBB",
    "homepage": "",
    "version": "1.0.0",
    "time": "2025-01-01",
    "keywords": [
        "phpbb",
        "extension",
        "captcha",
        "turnstile",
        "cloudflare",
        "anti-spam"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Maxim Maslich",
            "email": "mmaslich@gmail.com",
            "homepage": "",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0.0",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": "~3.3.0"
    },
    "require-dev": {
        "phing/phing": "^2.16.0",
        "pear/archive_tar": "^1.4.0"
    },
    "extra": {
        "display-name": "CloudFlare Turnstile",
        "soft-require": {
            "phpbb/phpbb": "~3.3.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/turnstile",
            "filename": "version_check",
            "ssl": true
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}